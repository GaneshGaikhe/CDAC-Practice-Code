//////////////reverse the array

public class Solution{
	public static void main(String[] args) {
		int [] arr= {1,5,6,5,5,55,8,9,7,6,4};
		
		int [] arr1=new int[arr.length];
		
		int j=0;
		for(int i=arr.length-1;i>=0;i--)
		{
			arr1[j]=arr[i];
			j++;
		}
		
		for(int i=0;i<arr1.length;i++)
		{
			System.out.println(arr1[i]);
		}
	}
}



