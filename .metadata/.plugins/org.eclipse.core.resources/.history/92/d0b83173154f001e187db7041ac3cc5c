class Solution {
    public int firstUniqChar(String s) {
        
        int count=0;
        char [] arr=String.toCharArray(s);
        for(int i=0;i<arr.length;i++)
        {
                for(int j=i+1;j<arr.length;j++)
                {
                    if(arr[i]==arr[j])
                    {

                        break;
                    }
                    return i;
                }
                count++;

        }
        if(count==arr.length)
        {
            return -1;
        }

    }
}